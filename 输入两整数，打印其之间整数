#include <iostream>
using namespace std;
int main()
{
  cout<<"请输入两个整数： "<<endl;
  int a=0,b=0;
  cin>>a>>b;
  if(a>b)
    {
      while(a>=b)
        {
          cout<<a<<endl;
          a--;
        }
    }
    else if(a<b)
    {
      while(a<=b)
        {
          cout<<b<<endl;
          b--;
        }
    }
    else if(a=b)
    {
     cout<<"没有合适的数"<<endl;
        
    }
    system("pause");
    return 0;
}
